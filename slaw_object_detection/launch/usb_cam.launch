<launch>
    <node pkg="usb_cam" name="usb_cam" type="usb_cam_node" respawn="false" output="screen">
        <param name="video_device" type="string" value="/dev/video-rv20" />
        <param name="image_width" type="int" value="640" />
        <param name="image_height" type="int" value="480" />
        <param name="pixel_format" type="string" value="yuyv"/>
    </node>
</launch>
